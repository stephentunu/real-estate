{
  "project": {
    "name": "jaston-real-estate",
    "display_name": "Jaston Real Estate Platform",
    "root_dir": "/home/douglas/jaston-real-estate",
    "user": "douglas",
    "group": "douglas"
  },
  "environment": {
    "DJANGO_SETTINGS_MODULE": "jaston.settings",
    "PYTHONPATH": "/home/douglas/jaston-real-estate/backend",
    "PATH": "/home/douglas/jaston-real-estate/backend/venv/bin:/usr/local/sbin:/usr/local/bin:/usr/sbin:/usr/bin:/sbin:/bin"
  },
  "logging": {
    "base_dir": "/var/log/jaston-real-estate",
    "max_size": "100M",
    "rotate_count": 10
  },
  "security": {
    "no_new_privileges": true,
    "protect_system": "strict",
    "protect_home": true,
    "private_tmp": true,
    "private_devices": true,
    "protect_kernel_tunables": true,
    "protect_kernel_modules": true,
    "protect_control_groups": true
  },
  "restart": {
    "policy": "always",
    "delay_sec": 3,
    "max_retries": 5
  },
  "resources": {
    "memory_limit": "512M",
    "cpu_quota": "100%",
    "tasks_max": 100
  }
}