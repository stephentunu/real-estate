{
  "environment": {
    "DJANGO_SETTINGS_MODULE": "jaston.settings",
    "DEBUG": "True",
    "LOG_LEVEL": "DEBUG"
  },
  "logging": {
    "base_dir": "/tmp/jaston-real-estate/logs"
  },
  "security": {
    "no_new_privileges": false,
    "protect_system": "false",
    "protect_home": false,
    "private_tmp": false,
    "private_devices": false
  },
  "resources": {
    "memory_limit": "256M",
    "cpu_quota": "50%",
    "tasks_max": 50
  },
  "restart": {
    "policy": "on-failure",
    "delay_sec": 1,
    "max_retries": 3
  }
}